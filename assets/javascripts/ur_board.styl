.ur_board
  .board
    text-align center
    position relative
    padding-bottom 20px
    .spacer
      height 50px
      padding 10px
    .board_message
      color white
    .board_overlay
      position: absolute
      width: 100%
      height: 100%
      background-color rgba(0, 0, 0, 0.3)
      box-shadow inset 0 0 10px 10px #555
      top: 0
      left: 0
      z-index: 2
