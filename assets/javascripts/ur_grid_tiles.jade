      use(xlink:href='#svg_five_circles', transform='translate(500, 500) scale(0.8) translate(-500, -500)')
    g#svg_five_circles_in_wavy(stroke-width='40')
      use(xlink:href='#svg_five_circles_in_wavy_quadrant', transform='translate(50, 50) scale(0.4)')
      use(xlink:href='#svg_five_circles_in_wavy_quadrant', transform='translate(50, 550) scale(0.4)')
      use(xlink:href='#svg_five_circles_in_wavy_quadrant', transform='translate(550, 50) scale(0.4)')
      use(xlink:href='#svg_five_circles_in_wavy_quadrant', transform='translate(550, 550) scale(0.4)')
      path(d='M 0 500 L 1000 500 M 500 0 L 500 1000', stroke-width='20')
