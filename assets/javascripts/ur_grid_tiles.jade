svg(xmlns='http://www.w3.org/2000/svg', width='0', height='0', viewbox='0 0 1000 1000', style='position: absolute;')
  defs
    filter#svg_displacementFilter
      feTurbulence(type='fractalNoise', basefrequency='0.01', numoctaves='2', result='turbulence')
      feDisplacementMap(in2='turbulence', in='SourceGraphic', scale='50', xchannelselector='R', ychannelselector='G')
    filter#svg_stainFilter
      feTurbulence(type='fractalNoise', basefrequency='0.03', numoctaves='3', result='turbulence')
      feColorMatrix(in='turbulence', type='saturate', values='0', result='bw')
      feDiffuseLighting(in='bw', result='light', lighting-color='#dcc7a9')
        feDistantLight(azimuth='240', elevation='20')
    rect#svg_bkg(x='0', y='0', width='1000', height='1000', style='filter: url(#svg_stainFilter)')
    // Eyes
    mask#svg_eye_mask(x='25', y='0', width='450', height='1000', maskcontentunits='userSpaceOnUse', maskunits='userSpaceOnUse')
      rect(x='25', y='0', width='450', height='1000', fill='white')
    g#svg_eye
      // size is 500x500
      circle(cx='250', cy='250', r='50', fill='#007')
      path(d='M 0 200 Q 150 100 250 100 Q 350 100 500 200')
      path(d='M 0 300 Q 150 400 250 400 Q 350 400 500 300')
      path(d='M 0 250 Q 150 150 250 150 Q 350 150 500 250 Q 350 350 250 350 Q 150 350 0 250 ')
