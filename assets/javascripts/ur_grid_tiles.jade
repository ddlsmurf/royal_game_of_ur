      path(d='M  35 0 L  35 550')
      path(d='M 465 0 L 465 550')
    g#svg_eyes_quadrant_top(mask='url(#svg_eye_mask)')
      use(xlink:href='#svg_eye', transform='translate(0 -50)')
      use(xlink:href='#svg_eyes_quadrant_side_lines')
    g#svg_eyes_quadrant_bottom(mask='url(#svg_eye_mask)')
      use(xlink:href='#svg_eye', transform='translate(0 50)')
      use(xlink:href='#svg_eyes_quadrant_side_lines')
    g#svg_eye_square_row
      rect(x=' 40', y='380', width='80', height='80')
      rect(x='165', y='380', width='80', height='80')
      rect(x='245', y='380', width='80', height='80')
      rect(x='375', y='380', width='80', height='80')
    g#svg_eyes_thing
      path(d='M 0 380 L 1000 380')
      use(xlink:href='#svg_eye_square_row', transform='')
      use(xlink:href='#svg_eye_square_row', transform='translate(0 160)')
      use(xlink:href='#svg_eye_square_row', transform='translate(500 0)')
      use(xlink:href='#svg_eye_square_row', transform='translate(500 160)')
      use(xlink:href='#svg_eyes_quadrant_top', transform='')
      use(xlink:href='#svg_eyes_quadrant_top', transform='translate(500 0)')
      use(xlink:href='#svg_eyes_quadrant_bottom', transform='translate(0 500)')
      use(xlink:href='#svg_eyes_quadrant_bottom', transform='translate(500 500)')
    use#svg_eyes_thing_bottom(xlink:href='#svg_eyes_thing', transform='translate(500 500) rotate(180) translate(-500 -500)')
    // Little squares
    g#svg_eyed_square
      rect(x='0', y='0', width='200', height='200')
      circle(cx='100', cy='100', r='50', fill='#007')
    g#svg_eyed_square_quadrant
      // size is 500x500
      rect(x='0', y='0', width='450', height='450')
      rect(x='0', y='250', width='200', height='200')
      use(xlink:href='#svg_eyed_square', transform='')
      use(xlink:href='#svg_eyed_square', transform='translate(250 0)')
      use(xlink:href='#svg_eyed_square', transform='translate(250 250)')
    g#svg_little_squares(transform='translate(500, 500) scale(0.9) translate(-500, -500)')
      rect(x='0', y='0', width='1000', height='1000')
