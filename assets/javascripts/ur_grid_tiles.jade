    g#svg_five_circles_in_wavy_in_wavy(transform='translate(500, 500) scale(0.9) translate(-500, -500)')
      use(xlink:href='#svg_wavy_box', transform='')
      use(xlink:href='#svg_five_circles_in_wavy_quadrant', stroke-width='30', transform='translate(500, 500) scale(0.6) translate(-500, -500)')
