    path#svg_wavy_box(fill='none', d='M0,71 L71,0 L214,143 L357,0 L500,143 L643,0 L786,143 L929,0 L1000,71 L857,214 L1000,357 L857,500 L1000,643 L857,786 L1000,929 L929,1000 L786,857 L643,1000 L500,857 L357,1000 L214,857 L71,1000 L0,929 L143,786 L0,643 L143,500 L0,357 L143,214 L0,71')
